{
    "name": "vscode-week-number",
    "displayName": "Week Number",
    "description": "Displays the current week number in the status bar",
    "version": "2.0.0",
    "publisher": "TomasHubelbauer",
    "repository": {
        "type" : "git",
        "url" : "https://github.com/TomasHubelbauer/vscode-week-number.git"
    },
    "engines": {
        "vscode": "^1.22.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.insertWeekNumber",
                "title": "Insert Current Week Number"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
